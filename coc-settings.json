{
	"languageserver": {
		"go": {
						"command": "gopls",
						"rootPatterns":["go.mod"],
						"trace.server": "verbose",
						"filetypes": ["go"]
		}
	},
	"snippets.ultisnips.directories": [
		"UltiSnips"
	],
	"snippets.ultisnips.usePythonx": true,
	"snippets.ultisnips.pythonVersion": 3,
	"go.enable": true,
	"json.enable": true,

	"go.goplsOptions": {
		"completeUnimported": true,
		"matcher": "Fuzzy"
	},
	"go.goplsUseDaemon": true,
	"go.trace.server": "verbose"
}
